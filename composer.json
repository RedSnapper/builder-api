{
  "name": "rs/builder-api",
  "description": "Make API calls to Builder",
  "license": "MIT",
  "authors": [
    {
      "name": "Joe Abousselam",
      "email": "joe.abousselam@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "RedSnapper\\Builder\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RedSnapper\\Builder\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.24",
    "phpunit/phpunit": "^9.5",
    "guzzlehttp/guzzle": "^7.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "RedSnapper\\Builder\\BuilderApiServiceProvider"
      ],
      "aliases": {
        "BuilderApi": "RedSnapper\\Builder\\Facades\\BuilderApi"
      }
    }
  }
}
